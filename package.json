{
  "title": "DOM Utilities",
  "name": "dom-utils",
  "description": "DOM Utilities",
  "version": "1.0.0",
  "homepage": "https://github.com/dwightjack/dom-utils",
  "author": "Marco Solazzi",
  "license": "MIT",
  "main": "dist/dom.js",
  "jsnext:main": "es/dom.js",
  "module": "es/dom.js",
  "files": [
    "dist",
    "es",
    "src",
    "umd"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dwightjack/dom-utils.git"
  },
  "bugs": "https://github.com/dwightjack/dom-utils/issues",
  "keywords": [
    "dom"
  ],
  "scripts": {
    "build": "npm run lint && npm run build:cjs && npm run build:es && cross-env NODE_ENV=production npm run build:umd",
    "build:es": "rimraf es && cross-env BABEL_ENV=es babel ./src -d es --ignore umd.js",
    "build:cjs": "rimraf dist && cross-env BABEL_ENV=cjs babel ./src -d dist --ignore umd.js",
    "build:umd": "rimraf umd && cross-env BABEL_ENV=rollup rollup -c rollup.config.js",
    "lint": "eslint -c ./src/.eslintrc.json src",
    "docs": "rimraf doc && node ./scripts/docs.js",
    "start": "node ./scripts/server.js",
    "test": "cross-env BABEL_ENV=rollup karma start karma.conf.js --single-run"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-es2015": "6.24.1",
    "cross-env": "5.0.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "2.7.0",
    "expect": "1.20.2",
    "expect-element": "1.1.1",
    "express": "4.15.3",
    "karma": "1.7.0",
    "karma-html2js-preprocessor": "1.1.0",
    "karma-mocha": "1.3.0",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-rollup-preprocessor": "4.0.0",
    "lodash": "4.17.4",
    "markdox": "0.1.10",
    "mocha": "3.4.2",
    "rimraf": "2.6.1",
    "rollup": "0.43.0",
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-commonjs": "8.0.2",
    "rollup-plugin-node-globals": "1.1.0",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-replace": "1.1.1",
    "rollup-plugin-uglify": "2.0.1",
    "serve-index": "1.9.0"
  },
  "dependencies": {
    "desandro-classie": "1.0.1"
  }
}
